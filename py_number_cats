print('How many cats do you have?')
numCats = input()
try:
    if int(numCats) >= 4:
        print('Thats a lot of cats.')
    elif int(numCats) >=0:   
        print('Thats not that many cats.')

    elif int(numCats) <= -1:
        print('Thats an imposible number of cats.')

except ValueError:
    print('You did not enter a number.')
